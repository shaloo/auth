{
  "name": "@arcana/auth",
  "version": "0.0.6",
  "description": "Arcana auth SDK for easier logins",
  "main": "dist/auth.umd.js",
  "jsdelivr": "dist/auth.umd.js",
  "unpkg": "dist/auth.umd.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:web",
    "build:types": "tsc --project tsconfig.json --emitDeclarationOnly",
    "build:web": "webpack",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "release": "np"
  },
  "np": {
    "branch": "main",
    "yarn": false
  },
  "author": "makyl<makyl@newfang.io>",
  "license": "ISC",
  "dependencies": {
    "@arcana/keystore": "^0.0.6",
    "@stablelib/base64": "^1.0.1",
    "ethers": "^5.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@peculiar/webcrypto": "^1.1.7",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.0",
    "husky": "^7.0.2",
    "jest": "^27.0.6",
    "jest-environment-jsdom": "^27.2.4",
    "jest-fetch-mock": "^3.0.3",
    "np": "^7.5.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.62.1",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  }
}
